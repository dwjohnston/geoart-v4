  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-regular-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "b": "^2.0.1",
    "blacksheep-geometry": "^1.0.0",
    "blacksheep-react-canvas": "2.0.2",
    "blacksheep-react-carousel": "1.0.1",
    "clipboard": "^2.0.1",
    "firebase": "^5.5.4",
    "geoplanets-model": "^1.0.2",
    "lodash": "^4.17.11",
    "mathjs": "^4.4.2",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-color": "^2.14.1",
    "react-compound-slider": "^0.12.0",
    "react-dom": "^16.5.2",
    "react-rgba-color-picker": "1.3.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-share": "^2.3.1",
    "react-tabs": "^2.3.0",
    "shortid": "^2.2.13"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "move-index": "mv ./build/index.html ./functions/index.html",
    "build": "npm-run-all build-css build-js move-index"
  },
  "proxy": {
    "/": {
      "target": "http://localhost:5000/geoart-v4/us-central1/app"
    }
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
